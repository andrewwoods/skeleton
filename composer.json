{
    "name": "awoods/skel",
    "description": "file generator for Skeleton",
        "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/andrewwoods/chicago-style"
        }
    ],
    "require": {
        "symfony/console": "^7.1",
        "andrewwoods/chicago-style": "^0.1.0",
        "twig/twig": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Skel\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2"
    }
}
